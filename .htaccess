RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^([^/]+)/$ $1.html [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^doc-coopbot/commands/ /doc-coopbot/404 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^doc-coopbot/systems/ /doc-coopbot/404 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^doc-coopbot/helps/ /doc-coopbot/404 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^doc-coopbot/infos/ /doc-coopbot/404 [R=301,L]

ErrorDocument 404 /doc-coopbot/404